{
  "name": "tessel",
  "productName": "Tessel",
  "version": "1.2.0",
  "description": "Desktop app for creating mosaic videos",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": " nodemon --exec electron .",
    "package-mac": "npx @electron/packager . --overwrite --platform=darwin --arch=arm64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "npx @electron/packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Tessel\"",
    "package-linux": "npx @electron/packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=false --out=release-builds"
  },
  "author": "James Sorbello",
  "license": "MIT",
  "devDependencies": {
    "electron": "^37.1.0",
    "@electron/packager": "^18.3.6",
    "nodemon": "^3.1.4"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "slash": "^5.1.0"
  }
}
